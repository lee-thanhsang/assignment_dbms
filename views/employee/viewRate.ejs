<body>
    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top">
      <div class="container d-flex align-items-center">
  
        <h1 class="logo me-auto"><a href="index.html"><span>B&K</span>Supermarket</a></h1>
        <!-- Uncomment below if you prefer to use an image logo -->
        <!-- <a href="index.html" class="logo me-auto me-lg-0"><img src="../../company/assets/img/logo.png" alt="" class="img-fluid"></a>-->
  
        <nav id="navbar" class="navbar order-last order-lg-0">
          <ul>
            <li><a href="home">Trang chủ</a></li>
            <li class="dropdown"><a href="#"><span>Thông tin </span> <i class="bi bi-chevron-down"></i></a>
              <ul>
                <li><a href="profile">Thông tin cá nhân</a></li>
                <li><a href="history">Lịch sử tư vấn</a></li>
                <li><a href="employee-information">Thông tin nhân viên</a></li>
              </ul>
            </li>
            <li><a href="promotion">Khuyến mãi</a></li>
            
            <li class="dropdown"><a href="#" class="active"><span>Khách hàng</span> <i class="bi bi-chevron-down"></i></a>
              <ul>
                <li><a href="customer-rate"><span>Đánh giá từ khách hàng</span></a></li>
                <li><a href="customer-information">Thông tin khách hàng</a></li>
              </ul>
            </li>

            <li class="dropdown"><a href="#"><span>Câu hỏi cần trả lời</span> <i class="bi bi-chevron-down"></i></a>
              <ul>
                <li><a href="answer-question-customer">Từ khách hàng</a></li>
                <li><a href="answer-question-supplier">Từ nhà cung cấp</a></li>
              </ul>
            </li>
            <li><a href="/logout">Đăng xuất</a></li>
          </ul>
          <i class="bi bi-list mobile-nav-toggle"></i>
        </nav><!-- .navbar -->
  
        <div class="header-social-links d-flex">
          <a href="#" class="twitter"><i class="bu bi-twitter"></i></a>
          <a href="#" class="facebook"><i class="bu bi-facebook"></i></a>
          <a href="#" class="instagram"><i class="bu bi-instagram"></i></a>
          <a href="#" class="linkedin"><i class="bu bi-linkedin"></i></i></a>
        </div>
  
      </div>
    </header><!-- End Header -->

  <main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="container">

        <div class="d-flex justify-content-between align-items-center">
          <h2>Lịch sử đánh giá</h2>
        </div>

      </div>
    </section><!-- End Breadcrumbs -->

    <!-- ======= Portfolio Section ======= -->
    <section id="portfolio" class="portfolio">
      <div class="container">

        <div class="row" data-aos="fade-up">
          <div class="col-lg-12 d-flex justify-content-center">
            <ul id="portfolio-flters">
              <li data-filter="*" class="filter-active">Tất cả đánh giá</li>
            </ul>
          </div>
        </div>

        <div class="row portfolio-container" data-aos="fade-up">
          <%rating.forEach(function(item){%>
            <div class="col-lg-3 col-md-6 aos-init aos-animate portfolio-item filter-employee">
                <div class="testimonial-item" style="text-align: center;height: 300px; padding: 5px; background-color: #E0FFFF;">
                  <p>Người đánh giá</p>
                  <h3> <%=item.LastName + " " + item.FirstName%></h3>
                  <h5 style="color: #1b67bd;"><%=item.Point_for_quality%><span> điểm Chất lượng</span></h5>
                  <h5 style="color: #1b67bd;"><%=item.Point_for_attitude%><span> điểm Thái độ</span></h5>
                  <p style="color: red;"><%=item.Time.toISOString().slice(0, 10)%></p>
                  <p style="text-align:left;"><%=item.Detail%></p>
                </div>
              </div>   
          <%})%>

      </div>
    </section>
    <!--End Portfolio Section -->

  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  <%- include ('_layouts/footer') -%>
  <!-- End Footer -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="../../company/assets/vendor/aos/aos.js"></script>
