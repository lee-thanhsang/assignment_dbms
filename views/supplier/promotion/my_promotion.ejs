<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>Khuyến mãi của tôi</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="../../company/assets/img/favicon.png" rel="icon">
  <link href="../../company/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="../../company/assets/vendor/animate.css/animate.min.css" rel="stylesheet">
  <link href="../../company/assets/vendor/aos/aos.css" rel="stylesheet">
  <link href="../../company/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="../../company/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="../../company/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="../../company/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="../../company/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="../../company/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">
  
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
  <!-- Template Main CSS File -->
  <link href="../../company/assets/css/style.css" rel="stylesheet">
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top">
      <div class="container d-flex align-items-center">
  
        <h1 class="logo me-auto"><a href="index.html"><span>B&K</span>Supermarket</a></h1>
        <!-- Uncomment below if you prefer to use an image logo -->
        <!-- <a href="index.html" class="logo me-auto me-lg-0"><img src="../../company/assets/img/logo.png" alt="" class="img-fluid"></a>-->
  
        <nav id="navbar" class="navbar order-last order-lg-0">
          <ul>
            <li><a href="/supplier/home">Trang chủ</a></li>
            <li class="dropdown"><a href="#"><span>Thông tin cá nhân</span> <i class="bi bi-chevron-down"></i></a>
              <ul>
                <li><a href="/supplier/profile">Thông tin cá nhân</a></li>
                <li><a href="/supplier/history">Lịch sử tương tác</a></li>
              </ul>
            </li>
            <li class="dropdown"><a href="#"  class="active"><span>Thông tin khuyến mãi</span> <i class="bi bi-chevron-down"></i></a>
              <ul>
                <li><a href="/supplier/promotion">Tất cả khuyến mãi</a></li>
                <li><a href="/supplier/my-promotion">Khuyến mãi của tôi</a></li>
              </ul>
            </li>
  
            <li><a href="/supplier/contact">Liên hệ</a></li>
            <li><a href="/logout">Đăng xuất</a></li>
          </ul>
          <i class="bi bi-list mobile-nav-toggle"></i>
        </nav><!-- .navbar -->
  
        <div class="header-social-links d-flex">
          <a href="#" class="twitter"><i class="bu bi-twitter"></i></a>
          <a href="#" class="facebook"><i class="bu bi-facebook"></i></a>
          <a href="#" class="instagram"><i class="bu bi-instagram"></i></a>
          <a href="#" class="linkedin"><i class="bu bi-linkedin"></i></i></a>
        </div>
  
      </div>
    </header><!-- End Header -->

  <main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="container">

        <div class="d-flex justify-content-between align-items-center">
          <h2>Khuyến mãi của tôi</h2>
          <button style="margin-left: 15%;" type="button" class="btn btn-primary" data-toggle="modal" data-target="#add-promotion-modal">Thêm khuyến mãi</button>
        </div>
      </div>
      
    </section><!-- End Breadcrumbs -->

    <!-- End Breadcrumbs -->

    <!-- ======= Blog Section ======= -->
    <section id="portfolio" class="portfolio">
      <div class="container">

        <div class="row" data-aos="fade-up">         
          <div class="col-lg-12 d-flex justify-content-center">
            <ul id="portfolio-flters">
              <li data-filter="*" class="filter-active">Tất cả</li>
              <li data-filter=".filter-food">Thực phẩm</li>
              <li data-filter=".filter-houseware">Đồ gia dụng</li>
              <li data-filter=".filter-fashion">Thời trang</li>
              <li data-filter=".filter-elec">Điện tử - Điện lạnh</li>
            </ul>
          </div>
        </div>

        <div class="row portfolio-container" data-aos="fade-up">
          <%food.forEach(function(item){%>
            <div class="col-lg-4 col-md-6 portfolio-item filter-food">
              <img src="../../company/assets/img/portfolio/food.jpg" class="img-fluid" alt="" style="height: 300px; width: 100%;">
              <div class="portfolio-info">
                <h4><%=item.PName + " - " + item.PType%></h4>
                <p>Thực phẩm - <span><%=item.Information%></span></p>
                <p>Từ <span style="color: red;"><%=item.FromDate.toISOString().slice(0, 10)%></span> đến hết <span style="color: red;"><%=item.ToDate.toISOString().slice(0, 10)%></span></p>
                <p>Thông tin chi tiết: <%=item.Infomation%></p>
                <a href="#" style="color: gray;">Giảm giá lên đến <span style="color: red;"><%=item.NumofPercentRedution%>%</span></as>
                <button data-pid="<%=item.PID%>" data-pname="<%=item.PName%>" data-gname="<%=item.GName%>" data-PType="<%=item.PType%>" data-FromDate="<%=item.FromDate.toISOString().slice(0, 10)%>" data-ToDate="<%=item.ToDate.toISOString().slice(0, 10)%>" data-Detail="<%=item.Infomation%>" Data-Percent="<%=item.NumofPercentRedution%>" title="Chỉnh sửa" style="margin-left: 10%;" type="button" class="btn update-promotion-button" data-toggle="modal" data-target="#update-promotion-modal"><i style="color: blue;" class="bi bi-pencil-square"></i></button>
                <button data-pid="<%=item.PID%>" title="Xóa" type="button" class="btn delete-button" data-toggle="modal" data-target="#promotion-delete-modal"><i style="color: red;" class="bi bi-trash-fill"></i></button>
              </div>
            </div>
          <%})%>

          <%fashion.forEach(function(item){%>
            <div class="col-lg-4 col-md-6 portfolio-item filter-fashion">
              <img src="../../company/assets/img/portfolio/fashion.png" class="img-fluid" alt="" style="height: 300px; width: 100%;">
              <div class="portfolio-info">
                <h4><%=item.PName + " - " + item.PType%></h4>
                <p>Thời trang - <span><%=item.Information%></span></p>
                <p>Từ <span style="color: red;"><%=item.FromDate.toISOString().slice(0, 10)%></span> đến hết <span style="color: red;"><%=item.ToDate.toISOString().slice(0, 10)%></span></p>
                <p>Thông tin chi tiết: <%=item.Infomation%></p>
                <a href="#" style="color: gray;">Giảm giá lên đến <span style="color: red;"><%=item.NumofPercentRedution%>%</span></as>
                <button data-pid="<%=item.PID%>" data-pname="<%=item.PName%>" data-gname="<%=item.GName%>" data-ptype="<%=item.PType%>" data-FromDate="<%=item.FromDate.toISOString().slice(0, 10)%>" data-ToDate="<%=item.ToDate.toISOString().slice(0, 10)%>" data-Detail="<%=item.Infomation%>" Data-Percent="<%=item.NumofPercentRedution%>" title="Chỉnh sửa" style="margin-left: 10%;" type="button" class="btn update-promotion-button" data-toggle="modal" data-target="#update-promotion-modal"><i style="color: blue;" class="bi bi-pencil-square"></i></button>
                <button data-pid="<%=item.PID%>" title="Xóa" type="button" class="btn delete-button" data-toggle="modal" data-target="#promotion-delete-modal"><i style="color: red;" class="bi bi-trash-fill"></i></button>
              </div>
            </div>
          <%})%>

          <%warehouse.forEach(function(item){%>
            <div class="col-lg-4 col-md-6 portfolio-item filter-houseware">
              <img src="../../company/assets/img/portfolio/houseware.png" class="img-fluid" alt="" style="height: 300px; width: 100%;">
              <div class="portfolio-info">
                <h4><%=item.PName + " - " + item.PType%></h4>
                <p>Đồ gian dụng - <span><%=item.Information%></span></p>
                <p>Từ <span style="color: red;"><%=item.FromDate.toISOString().slice(0, 10)%></span> đến hết <span style="color: red;"><%=item.ToDate.toISOString().slice(0, 10)%></span></p>
                <p>Thông tin chi tiết: <%=item.Infomation%></p>
                <a href="#" style="color: gray;">Giảm giá lên đến <span style="color: red;"><%=item.NumofPercentRedution%>%</span></as>
                <button data-pid="<%=item.PID%>" data-pname="<%=item.PName%>" data-gname="<%=item.GName%>" data-PType="<%=item.PType%>" data-FromDate="<%=item.FromDate.toISOString().slice(0, 10)%>" data-ToDate="<%=item.ToDate.toISOString().slice(0, 10)%>" data-Detail="<%=item.Infomation%>" Data-Percent="<%=item.NumofPercentRedution%>" title="Chỉnh sửa" style="margin-left: 10%;" type="button" class="btn update-promotion-button" data-toggle="modal" data-target="#update-promotion-modal"><i style="color: blue;" class="bi bi-pencil-square"></i></button>
                <button data-pid="<%=item.PID%>" title="Xóa" type="button" class="btn delete-button" data-toggle="modal" data-target="#promotion-delete-modal"><i style="color: red;" class="bi bi-trash-fill"></i></button>
              </div>
            </div>
          <%})%>

          <%electronic.forEach(function(item){%>
            <div class="col-lg-4 col-md-6 portfolio-item filter-elec">
              <img src="../../company/assets/img/portfolio/dtdl.jpg" class="img-fluid" alt="" style="height: 300px; width: 100%;">
              <div class="portfolio-info">
                <h4><%=item.PName + " - " + item.PType%></h4>
                <p>Điện tử/Điện lạnh - <span><%=item.Information%></span></p>
                <p>Từ <span style="color: red;"><%=item.FromDate.toISOString().slice(0, 10)%></span> đến hết <span style="color: red;"><%=item.ToDate.toISOString().slice(0, 10)%></span></p>
                <p>Thông tin chi tiết: <%=item.Infomation%></p>
                <a href="#" style="color: gray;">Giảm giá lên đến <span style="color: red;"><%=item.NumofPercentRedution%>%</span></as>
                <button data-pid="<%=item.PID%>" data-pname="<%=item.PName%>" data-PType="<%=item.PType%>" data-FromDate="<%=item.FromDate.toISOString().slice(0, 10)%>" data-ToDate="<%=item.ToDate.toISOString().slice(0, 10)%>" data-Detail="<%=item.Infomation%>" Data-Percent="<%=item.NumofPercentRedution%>" title="Chỉnh sửa" style="margin-left: 10%;" type="button" class="btn update-promotion-button" data-toggle="modal" data-target="#update-promotion-modal"><i style="color: blue;" class="bi bi-pencil-square"></i></button>
                <button data-pid="<%=item.PID%>" title="Xóa" type="button" class="btn delete-button" data-toggle="modal" data-target="#promotion-delete-modal"><i style="color: red;" class="bi bi-trash-fill"></i></button>
              </div>
            </div>

          <%})%>

        </div>
          
      </div>
    </section><!-- End Blog Section -->

    <section>
      <!-- add modal -->
      <div class="modal fade" id="add-promotion-modal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header" style="background-color: #0d6eff; color: white">
              <h5 class="modal-title" id="exampleModalLabel" >Thêm khuyến mãi</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
              <form action="add-promotion" method="post">
                <div class="form-group">
                  
                  <div class="row">
                    
                    <div class="col-md-12">
                      <label>Tên khuyến mãi</label>
                      <input type="text" class="form-control" name="PName" required>
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <div class="row">
                    <div class="col-md-6">
                      <label>Mã số khyến mãi</label>
                      <input type="text" class="form-control" name="PID" required>
                    </div>
                    <div class="col-md-6">
                      <label>Loại khuyến mãi</label>
                      <input type="text" class="form-control" name="Type" placeholder="Sự kiện 11/11" required>
                    </div>
                   
                  </div>
                </div>

                <div class="form-group" style="margin-top: 5%;">                  
                  <div class="row">
                    <div class="col-md-6">
                      <label>Từ ngày</label>
                      <input type="text" class="form-control" placeholder="vd: 2021-10-30" name="FromDate" required>
                    </div>
                    <div class="col-md-6">
                      <label>Đến ngày</label>
                      <input type="text" class="form-control" placeholder="vd: 2021-10-31" name="ToDate" required>
                    </div>
                  </div>
                </div>

                <div class="form-group" style="margin-top: 5%;">                  
                  <div class="row">
                    <div class="col-md-6">
                      <label>Phần trăm giảm giá</label>
                      <input type="text" class="form-control" placeholder="Vd: 50" name="Percent" required>
                    </div>
                    <div class="col-md-6">
                      <label>Loại sản phẩm</label>
                      <select type="text" class="form-control" name="GName">
                        <%Goods.forEach(function(good){%>
                          <option value="<%=good.Name%>"><%=good.Information%></option>
                        <%})%>
                      </select>
                    </div>
                  </div>
                </div>

                <div class="form-group" style="margin-top: 5%;">  
                                  
                  <div class="row">
                    <div class="col-md-12">
                      <label>Thông tin chi tiết</label>
                      <input type="text" class="form-control" placeholder="Chi tiết" name="Detail" required>
                    </div>
                  
                  </div>
                </div>

                <div class="row">
                  <div class="col-md-6">
                  </div>
                  <div class="col-md-6">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal" style="margin-left: 5%;">Đóng</button>
                    <button type="submit" class="btn btn-primary" style="margin-left: 10%;">Thêm</button>
                  </div>
                </div>
              </form>
            </div>
            <div class="modal-footer">
            </div>
          </div>
        </div>
      </div>

      <!-- fashion modal -->
      <div class="modal fade" id="update-promotion-modal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header" style="background-color: #0d6eff;">
              <h5 class="modal-title" id="exampleModalLabel" >Chỉnh sửa khuyến mãi</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
              <form action="update-promotion" method="post">
                <div class="form-group">
                  
                  <div class="row">
                    <div class="col-md-6">
                      <label>Tên khuyến mãi</label>
                      <input type="text" class="form-control" name="PName" required>
                      <input type="text" class="form-control" name="PID" hidden=true>
                    </div>
                    <div class="col-md-6">
                      <label>Loại khuyến mãi</label>
                      <input type="text" class="form-control" name="PType" placeholder="Sự kiện 11/11" required>
                    </div>
                  </div>
                </div>

                <div class="form-group" style="margin-top: 5%;">                  
                  <div class="row">
                    <div class="col-md-6">
                      <label>Từ ngày</label>
                      <input type="text" class="form-control" placeholder="vd: 2021-10-30" name="FromDate" required>
                    </div>
                    <div class="col-md-6">
                      <label>Đến ngày</label>
                      <input type="text" class="form-control" placeholder="vd: 2021-10-31" name="ToDate" required>
                    </div>
                  </div>
                </div>

                <div class="form-group" style="margin-top: 5%;">                  
                  <div class="row">
                    <div class="col-md-6">
                      <label>Phần trăm giảm giá</label>
                      <input type="text" class="form-control" placeholder="Vd: 50" name="Percent" required>
                    </div>
                    <div class="col-md-6">
                      <label>Loại hàng hóa</label>
                      <select type="text" class="form-control" name="GName">
                        <%Goods.forEach(function(good){%>
                          <option value="<%=good.Name%>"><%=good.Information%></option>
                        <%})%>
                      </select>
                    </div>
                  </div>
                </div>

                <div class="form-group" style="margin-top: 5%;">  
                                  
                  <div class="row">
                    <div class="col-md-12">
                      <label>Thông tin chi tiết</label>
                      <input type="text" class="form-control" placeholder="Chi tiết" name="Detail" required>
                    </div>
                  
                  </div>
                </div>

                <div class="row">
                  <div class="col-md-6">
                  </div>
                  <div class="col-md-6">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal" style="margin-left: 5%;">Đóng</button>
                    <button type="submit" class="btn btn-primary" style="margin-left: 10%;">Cập nhật</button>
                  </div>
                </div>
              </form>
            </div>
            <div class="modal-footer">
            </div>
          </div>
        </div>
      </div>

      <!-- delete modal -->
      <div class="modal fade" id="promotion-delete-modal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header" style="background-color: red; color: white">
              <h5 class="modal-title" id="exampleModalLabel" >Xóa khuyến mãi</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
              <label>Bạn muốn xóa khuyến mãi này?</label>
              <form action="delete-promotion" method="post">
                <div class="row">
                  <div class="col-md-6">
                    <input type="text" class="form-control" name="PID" hidden>
                  </div>
                  <div class="col-md-6">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal" style="margin-left: 5%;">Đóng</button>
                    <button type="submit" class="btn" style="margin-left: 10%; background-color: red; color: white">Xóa</button>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>


    </section>
  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  <%- include ('../_layouts/footer') -%>
  <!-- End Footer -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="../../company/assets/vendor/aos/aos.js"></script>
  <script src="../../company/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="../../company/assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="../../company/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="../../company/assets/vendor/php-email-form/validate.js"></script>
  <script src="../../company/assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="../../company/assets/vendor/waypoints/noframework.waypoints.js"></script>

  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>

  <!-- Template Main JS File -->
  <script src="../../company/assets/js/main.js"></script>

  <script src="../../js/supplier/index.js"></script>

</body>

</html>