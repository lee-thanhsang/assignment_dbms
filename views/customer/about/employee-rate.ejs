            <div class="col-lg-6" style="background-color: #e9ecef; padding: 20px;">
              <form action="/rate-employee" method="post" >
                <div class="row">
                  <div class="form-group col-md-4" style="margin-top: 20px;">
                    <label for="inputState">Mã số Nhân viên</label>
                    <select id="inputState" class="form-control" name="employee">
                      <%employee_info.forEach(function(item){%>
                        <option value=<%=item.SSN%>><%=item.SSN +" - "+ item.ELastName+" "+item.EFirstName%></option>
                      <%})%>
                    </select>
                  </div>
                  <div class="form-group col-md-4" style="margin-top: 20px;">
                    <label for="inputState">Điểm chất lượng</label>
                    <select id="inputState" class="form-control" name="point_quality">
                      <option value=1>1 điểm</option>
                      <option value=2>2 điểm</option>
                      <option value=3>3 điểm</option>
                      <option value=4>4 điểm</option>
                      <option value=5>5 điểm</option>
                      <option value=6>6 điểm</option>
                      <option value=7>7 điểm</option>
                      <option value=8>8 điểm</option>
                      <option value=9>9 điểm</option>
