                  <label for="inputAddress">Nội dung</label>
                  <input name="message" type="text" class="form-control" id="inputAddress" placeholder="Nhập nội dung đánh giá" required>
                </div>
                <button style="margin-top: 30px; margin-left: 40%;" type="submit" class="btn btn-primary">Gửi đánh giá</button>
